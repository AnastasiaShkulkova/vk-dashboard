ormal;\r\n  font-weight: bold;\r\n  font-size: 30px;\r\n  line-height: 36px;\r\n  color: $primary_font_color;\r\n  align-self: center;\r\n}\r\n\r\n.header__search-form {\r\n    display: flex;\r\n    max-height: 35px;\r\n    align-self: center;\r\n    grid-gap: 15px;\r\n    border: none;\r\n}\r\n\r\n.header__search {\r\n    border-radius: 50px;\r\n    border: 0;\r\n}\r\n\r\n.header__notification-btn {\r\n    background: none;\r\n    border: none;\r\n}\r\n\r\n.header__profile {\r\n    display: flex;\r\n    justify-self: end;\r\n    align-self: center;\r\n    grid-gap: 10px;\r\n    margin-right: 50px;\r\n}\r\n\r\n.header__user-name {\r\n    font-family: $primary_font;\r\nfont-style: normal;\r\nfont-weight: 600;\r\nfont-size: 20px;\r\nline-height: 24px;\r\ndisplay: flex;\r\nalign-items: center;\r\ncolor: $primary_font_color;\r\n}\r\n\r\n.header__dropdown-btn {\r\n    background: none;\r\n    border: none;\r\n}","scss\\components\\_nav.scss":".nav {\r\n  background-color: #fdfdff;\r\nborder: 2px solid #EBEBEB;\r\n  display: grid;\r\n  grid-area: nav;\r\n  border-radius: $rounded 0 0 $rounded;\r\n}\r\n\r\n.nav__logo {\r\ndisplay: grid;\r\n\r\n}\r\n\r\n.logo__item {\r\n\r\nwidth: 42px;\r\nheight: 24px;\r\njustify-self: center;\r\nalign-self: center;\r\n\r\n}\r\n\r\n.nav__link {\r\n    display: grid;\r\n    grid-column: 1;\r\njustify-self: center;\r\n    grid-gap: 40px;\r\n}\r\n\r\n.nav__toogle-theme {\r\n    margin-bottom: 50px;\r\n    padding: 5px 30px 1px 8px;\r\n    justify-self: center;\r\n    align-self: end;\r\n    border: 1px solid black;\r\n    border-radius: 50px;\r\n}","scss\\components\\_main.scss":".main {\r\n    padding: 20px 30px;\r\n    background-color: blueviolet;\r\n    grid-area: main;\r\n    border-radius: 0 0 $rounded 0;\r\n    grid-gap: 35px;\r\n    \r\n\r\n}","scss\\components\\_info-card.scss":".info__cards {\r\n  display: grid;\r\n  grid-template-columns: repeat(3, 1fr);\r\n  grid-template-rows: repeat(1, 150px);\r\n  grid-gap: 30px;\r\n}\r\n\r\n.card {\r\n  padding: 25px;\r\n  background-color: $primary_bg;\r\n  display: grid;\r\n  grid-template-columns: 50% 50%;\r\n  grid-template-rows: auto auto;\r\n  grid-gap: 35px 0;\r\n  border-radius: $rounded;\r\n}\r\n\r\n.card__title {\r\n  font-family: $primary_font;\r\n  font-style: normal;\r\n  font-weight: bold;\r\n  font-size: 20px;\r\n  line-height: 24px;\r\n  color: $primary_font_color;\r\n}\r\n\r\n.card__menu {\r\n  justify-self: end;\r\n}\r\n\r\n.card__statistics {\r\n  justify-self: end;\r\n}\r\n\r\n.card__info {\r\n  font-family: $primary_font;\r\n  font-style: normal;\r\n  font-weight: 500;\r\n  font-size: 20px;\r\n  line-height: 24px;\r\n  color: $primary_font_color;\r\n}\r\n\r\n.card__statistics {\r\n  font-family: $primary_font;\r\n  font-style: normal;\r\n  font-weight: 500;\r\n  font-size: 20px;\r\n  line-height: 24px;\r\n  color: $secondary_font_color;\r\n}\r\n\r\n\r\n","scss\\components\\_statistics-cards.scss":".statistics__cards {\r\n  margin-top: 30px;\r\n  display: grid;\r\n  grid-template-columns: repeat(2, 1fr);\r\n  grid-template-rows: repeat(2, 250px);\r\n  grid-gap: 30px;\r\n}\r\n\r\n.card__digram {\r\n  background-color: $primary_bg;\r\n  padding: 25px;\r\n  border-radius: $rounded;\r\n}\r\n\r\n.audience__header {\r\n  display: grid;\r\n  grid-template-columns: repeat(3, 1fr);\r\n  align-items: center;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.audience__header--gender {\r\n  display: grid;\r\n  grid-template-columns: repeat(2, 1fr);\r\n}\r\n\r\n.card__header {\r\n  padding-bottom: 20px;\r\n  display: flex;\r\n  justify-content: space-between;\r\n}\r\n\r\n.card__switcher {\r\n  display: flex;\r\n}\r\n\r\n.switcher__item-active {\r\n  padding-right: 15px;\r\n  font-family: $primary_font;\r\n  font-style: normal;\r\n  font-weight: 600;\r\n  font-size: 12px;\r\n  line-height: 14px;\r\n  display: flex;\r\n  align-items: center;\r\n  text-align: right;\r\n  color: $primary_font_color;\r\n}\r\n\r\n.switcher__item {\r\n  font-family: $primary_font;\